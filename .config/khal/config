[calendars]

  [[personal]]
  path = ~/Documents/.calendars/personal
  priority = 100
  color = dark green

  [[work]]
  path = ~/Documents/.calendars/work
  priority = 90
  color = dark red

  [[school]]
  path = ~/Documents/.calendars/school
  priority = 80
  color = dark magenta

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M

[default]
default_calendar = personal
print_new = event
timedelta = 31d
highlight_event_days = True
